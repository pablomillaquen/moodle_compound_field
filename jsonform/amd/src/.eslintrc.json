{
    "ecmaFeatures": {
        "modules": true,
        "spread" : true,
        "restParams" : true
    },
    "env" : {
        "browser" : true,
        "node" : true,
        "es6" : true
    },
    "rules" : {
        "no-tabs": ["error",{"allowIndentationTabs": true}],
       "no-unused-vars": "off",
       "no-debugger":"off"
    },
    "parserOptions": {
        "sourceType": "module"
    }
}